Departments
-
dept_no pk varchar fk - dept_emp.dept_no
dept_name Varchar

dept_manager
-
dept_no pk verchar fk - departments.dept_no
emp_no int pk fk - employees.emp_no
from_date date
to_date date

dept_emp
-
emp_no pk int fk - salaries.emp_no
dept_no pk varchar
from_date
to_date

employees
-
emp_no pk int fk - dept_emp.emp_no
birth_date date
first_name varchar
last_name varchar
gender varchar
hire_date date

salaries
-
emp_no pk int fk - titles.emp_no
salary int
from_date date
to_date date

titles
-
emp_no pk int fk - employees.emp_no 
title pk varchar
from_date pk date
to_date date
